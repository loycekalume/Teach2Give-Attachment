{
  "compilerOptions": {
  "target": "ESNext",
 "module": "CommonJS",
  "moduleResolution": "node",
  "outDir": "dist",
  "rootDir": "src",
  "strict": true,
  "esModuleInterop": true,
  "forceConsistentCasingInFileNames": true,
  "skipLibCheck": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
  }